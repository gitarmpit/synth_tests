#graph {
	position: relative;
	float: left;
	width: 475px;
	height: 225px;
	margin: 5px 10px 5px 0px;
	padding: 3px;
	border: 1px solid #888;
}
#grid, #plot {
	position: absolute;
	top: 0px;
	left: 0px;
}
.allcontrols {
	float: left;
	clear: both;
}
.slider {
	float: left;
	width: 45px;
	background-color: #EEE;
	border: 1px solid #DDD;
}
.slider:first-child {
	background-color: #DEA;
}
.slider button {
	width: 34px;
	text-align: center;
	margin: 2px 5px;
}
.value {
	float: left;
	width: 34px;
	margin: 5px 2px;
	padding: 2px 2px 2px 4px;
	background-color: #FFF;
	box-shadow: inset 1px 1px 2px #AAA;
	font-size: 0.85em;
}
.scrollbar {
	float: left;
	width: 5px;
	height: 100px;
	margin: 5px 20px;
	background-color: #000;
	border: 1px solid #FFF;
}
.slidebutton {
	position: relative;
	top: 75px;
	left: -7px;
	width: 17px;
	height: 3px;
	border-width: 5px 1px;
	border-color: #88A;
	border-style: solid;
	background-color: #FFF;
	box-shadow: 2px 2px 2px #888;
}
.slidetitle {
	width: 45px;
	float: left;
	clear: both;
	margin: 5px 0px;
	text-align: center;
}
#webapi {
	float: left;
	margin-left: 10px;
	padding: 8px;
	width: 140px;
	height: auto;
	border: 3px solid #888;
	border-radius: 7px;
	box-shadow: 3px 3px 3px #AAA;
}
#webapi button {
	float: left;
	margin: 2px 5px;
	}
#webapi h4 {
	clear: both;
	}
#amplval {
	float: left;
	position: relative;
	width: 40px;
	height: 17px;
	margin: 5px 2px;
	background-color: #FFF;
	box-shadow: inset 1px 1px 2px #AAA;
	font-size: 0.85em;
}
#amplvalbar, #amplvalnum {
	position: absolute;
	top: 0px;
	left: 0px;
	height: 17px;
	}
#amplvalbar {
	width: 0px;
	background-color: #44C;
	opacity:0.3;
	}
#amplvalnum {
	padding: 2px 0px 0px 10px;
	}